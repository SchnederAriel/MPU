<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/png" sizes="32x32" href="~/assets/ico/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/assets/ico/favicon-16x16.png">

    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#00519a">

    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />

    <title>@ViewBag.Title - CUNA</title>

    @*<title>@ViewBag.Title - My ASP.NET Application</title>*@
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    @Styles.Render("~/Content/datepicker")
    @Styles.Render("~/Content/grilla")
    @Styles.Render("~/Content/fontawesome")

    @Scripts.Render("~/bundles/impresion")

    @RenderSection("estilos", required: false)

    <script type="text/javascript">
        var isIE = document.all && document.addEventListener && !window.atob;
    </script>


</head>
<body>
    @*<div class="container body-content">
            @RenderBody()
            <hr />
        </div>*@

    <div id="spinner-container">
        <div class="overlay">
            <div class="spinner__container">
                <div class="spinner__loading"></div>
                <div class="spinner__text"></div>
            </div>
        </div>
    </div>
    <main class="body-content">
        <div class="container">
            <div class="row">
                <div class="col-md-12" style="margin-top: 30px;">
                    @RenderBody()
                </div>
            </div>
        </div>
    </main>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@

    @Scripts.Render("~/bundles/datepicker")
    @Scripts.Render("~/bundles/inputmask")
    @Scripts.Render("~/bundles/moment")
    @Scripts.Render("~/bundles/grilla")
    @Html.RenderSectionEx("AvScripts")
    @Scripts.Render("~/bundles/funcionesglobales")
    @Scripts.Render("~/bundles/impresion")

    @RenderSection("scripts", required: false)
</body>
</html>